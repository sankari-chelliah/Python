class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        for i in range(0,len(nums)-1):
            j =i+1
            while j < len(nums):
                k=nums[i]+ nums[j]
                if k == target:
                    return(i,j)
                j=j+1
